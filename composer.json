{
    "name": "jorpo/resolution",
    "description": "Object resolver using Auryn dependency injection",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@designermonkey.co.uk"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "^8.0",
        "rdlowrey/auryn": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Jorpo\\Resolution\\": [
                "src"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Jorpo\\Resolution\\": [
                "tests"
            ]
        }
    }
}
